# Backend Server Configuration
PORT=3001
NODE_ENV=development

# MongoDB Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/easy-track?retryWrites=true&w=majority

# OpenAI/OpenRouter API Configuration
BOSS_AI_API_KEY=your_openrouter_api_key_here
# Using OpenRouter (free & open-source models)
# Get API key from: https://openrouter.ai/keys

# AI Model Configuration
AI_MODEL=openrouter/auto
# Alternative models:
# - openrouter/auto (best model auto-selection)
# - mistralai/mistral-7b-instruct
# - meta-llama/llama-2-70b-chat
# - gpt-3.5-turbo
# - gpt-4

# Supabase Configuration (if using Supabase from backend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# CORS Configuration
CORS_ORIGIN=http://localhost:5173,https://yourdomain.com

# Server Configuration
HOST=0.0.0.0
REQUEST_TIMEOUT=30000
MAX_REQUEST_SIZE=50mb

# Logging
LOG_LEVEL=info
